{
  "name": "quickpdf",
  "version": "1.0.0",
  "description": "application to minimize the build time of a pdf creation",
  "main": "./out/main/index.js",
  "author": "Ai Anshu",
  "homepage": "quickpdf.codenanshu.in",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "start": "electron-vite preview",
    "dev": "electron-vite dev",
    "build": "npm run typecheck && electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:win": "npm run build && electron-builder --win --config",
    "build:mac": "electron-vite build && electron-builder --mac --config",
    "build:linux": "electron-vite build && electron-builder --linux --config"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^1.0.3",
    "@electron-toolkit/utils": "^1.0.2",
    "@emotion/react": "^11.11.1",
    "@mantine/core": "^6.0.15",
    "@mantine/dates": "^6.0.15",
    "@mantine/form": "^6.0.15",
    "@mantine/hooks": "^6.0.15",
    "@mantine/notifications": "^6.0.15",
    "electron-updater": "^5.3.0",
    "firebase": "^9.23.0",
    "framer-motion": "^10.12.18",
    "grapesjs": "^0.21.2",
    "grapesjs-tui-image-editor": "^1.0.2",
    "html2canvas": "^1.4.1",
    "localforage": "^1.10.0",
    "match-sorter": "^6.3.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.14.1"
  },
  "devDependencies": {
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@electron/notarize": "^1.2.4",
    "@types/node": "16.18.21",
    "@types/react": "18.0.30",
    "@types/react-dom": "18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.60.1",
    "@typescript-eslint/parser": "^5.60.1",
    "@vitejs/plugin-react": "^3.1.0",
    "backbone": "^1.4.1",
    "electron": "^22.3.15",
    "electron-builder": "^23.6.0",
    "electron-vite": "^1.0.24",
    "eslint": "^8.44.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "grapesjs-blocks-basic": "^1.0.2",
    "grapesjs-cli": "^4.1.1",
    "lit-html": "^2.7.4",
    "prettier": "^2.8.8",
    "sass": "^1.63.6",
    "typescript": "^4.9.5",
    "vite": "^4.3.9"
  }
}
